<%
List<Role> userRoles = RoleLocalServiceUtil.getUserRoles(user2.getUserId());
%>
<c:if test="<%= userRoles != null && userRoles.size()>0 %>">
	<tr>
		<td>
			<liferay-ui:message key="roles" />
		</td>
		<td>
		<% for (int i=0; userRoles!=null && i<userRoles.size()-1; i++) { %>
			<%=userRoles.get(i).getName()%>, 
			<%--import "/html/portlet/enterprise_admin/user_role_action.jsp" --%>
		<% } %>
		<%=userRoles.get(userRoles.size()-1).getName()%>
		</td>
	</tr>
</c:if>