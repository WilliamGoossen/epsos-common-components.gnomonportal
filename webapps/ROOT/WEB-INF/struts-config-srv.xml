	<action path="/ext/srv_actions/list" type="com.ext.portlet.srv.actions.ListSrvServiceActionsAction" scope="request">
			<forward name="common.success" path="portlet.ext.srv.actions.list"  />
			<forward name="common.failure" path="/common/error.jsp"  />
		</action>

		<action path="/ext/srv_actions/load" type="com.ext.portlet.srv.actions.LoadSrvServiceActionAction"
