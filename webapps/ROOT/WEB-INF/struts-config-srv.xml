			<forward name="common.failure" path="/common/error.jsp"  />
		</action>	               	                  




<!-- USER REPORTS -->


<action path="/ext/srv_userreports/list" type="com.ext.portlet.srv.reports.user.ListSrvServiceUserReportsAction" scope="request">
			<forward name="common.success" path="portlet.ext.srv.reports.user.list"  />
