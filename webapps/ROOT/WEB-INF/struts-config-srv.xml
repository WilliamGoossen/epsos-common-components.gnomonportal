			<forward name="common.delete" path="portlet.ext.srv.srv_adminregistrations.form"  />
			<forward name="common.failure" path="/common/error.jsp"  />
		</action>

		<action path="/ext/srv_adminregistrations/add" type="com.ext.portlet.srv.registrations.admin.AddSrvServiceRegistrationAction"
		    name="SrvServiceAdminRegistrationForm" validate="false" scope="request">
			<forward name="/ext/srv_adminregistrations/list" path="/ext/srv_adminregistrations/list" redirect="true"/>
			<forward name="common.validation" path="portlet.ext.srv.srv_adminregistrations.form"  />
			<forward name="common.failure" path="/common/error.jsp"  />
