			<forward name="common.add" path="portlet.ext.srv.actions.form"  />
			<forward name="common.edit" path="portlet.ext.srv.actions.form"  />
			<forward name="common.delete" path="portlet.ext.srv.actions.form"  />
			<forward name="common.failure" path="/common/error.jsp"  />
		</action>

		<action path="/ext/srv_actions/add" type="com.ext.portlet.srv.actions.AddSrvServiceActionAction"
		    name="SrvServiceActionForm" validate="false" scope="request">
			<forward name="/ext/srv_actions/list" path="/ext/srv_actions/list" redirect="true"/>
			<forward name="common.validation" path="portlet.ext.srv.actions.form"  />
			<forward name="common.failure" path="/common/error.jsp"  />
		</action>

		<action path="/ext/srv_actions/update" type="com.ext.portlet.srv.actions.UpdateSrvServiceActionAction"
