<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

<form-beans>

<!-- Helpdesk Categories -->
<form-bean name="SrvServiceForm" type="com.ext.portlet.srv.SrvServiceForm"/>
<form-bean name="SrvServiceRegistrationForm" type="com.ext.portlet.srv.registrations.SrvServiceRegistrationForm"/>
<form-bean name="SrvServiceAdminRegistrationForm" type="com.ext.portlet.srv.registrations.admin.SrvServiceAdminRegistrationForm"/>
<form-bean name="SrvServiceReportForm" type="com.ext.portlet.srv.reports.SrvServiceReportForm"/>
<form-bean name="SrvServiceActionForm" type="com.ext.portlet.srv.actions.SrvServiceActionForm"/>
</form-beans>

<action-mappings>
	
	<!-- Services -->
	
		<action path="/ext/srv/list" type="com.ext.portlet.srv.ListSrvServicesAction" scope="request">
			<forward name="common.success" path="portlet.ext.srv.list"  />
			<forward name="common.failure" path="/common/error.jsp"  />
		</action>

		<action path="/ext/srv/load" type="com.ext.portlet.srv.LoadSrvServiceAction"
		    name="SrvServiceForm" validate="false" scope="request">
		    <forward name="common.view" path="portlet.ext.srv.form"  />
			<forward name="common.add" path="portlet.ext.srv.form"  />
			<forward name="common.edit" path="portlet.ext.srv.form"  />
			<forward name="common.delete" path="portlet.ext.srv.form"  />
			<forward name="common.failure" path="/common/error.jsp"  />
		</action>

		<action path="/ext/srv/add" type="com.ext.portlet.srv.AddSrvServiceAction"
		    name="SrvServiceForm" validate="false" scope="request">
			<forward name="/ext/srv/list" path="/ext/srv/list" redirect="true"/>
			<forward name="common.validation" path="portlet.ext.srv.form"  />
			<forward name="common.failure" path="/common/error.jsp"  />
		</action>

		<action path="/ext/srv/update" type="com.ext.portlet.srv.UpdateSrvServiceAction"
		    name="SrvServiceForm" validate="false" scope="request">
			<forward name="/ext/srv/list" path="/ext/srv/list" redirect="true"/>
			<forward name="common.validation" path="portlet.ext.srv.form"  />
			<forward name="common.failure" path="/common/error.jsp"  />
		</action>

		<action path="/ext/srv/delete" type="com.ext.portlet.srv.DeleteSrvServiceAction"
		    name="SrvServiceForm" validate="false" scope="request">
			<forward name="/ext/srv/list" path="/ext/srv/list" redirect="true"/>
			<forward name="common.validation" path="portlet.ext.srv.form"  />
			<forward name="common.failure" path="/common/error.jsp"  />
		</action>
	                   
	
	
	
	<!-- User Registration to Services -->
	
			<action path="/ext/srv_registrations/load" type="com.ext.portlet.srv.registrations.LoadSrvServiceRegistrationAction"
		    name="SrvServiceRegistrationForm" validate="false" scope="request">
		    <forward name="common.view" path="portlet.ext.srv.registrations.form"  />
			<forward name="common.add" path="portlet.ext.srv.registrations.form"  />
			<forward name="common.failure" path="/common/error.jsp"  />
		</action>

		<action path="/ext/srv_registrations/add" type="com.ext.portlet.srv.registrations.AddSrvServiceRegistrationAction"
		    name="SrvServiceRegistrationForm" validate="false" scope="request">
			<forward name="/ext/srv_registrations/load" path="/ext/srv_registrations/load" redirect="true"/>
			<forward name="srv.registrations.list" path="srv.registrations.list"/>
			<forward name="common.validation" path="portlet.ext.srv.registrations.form"  />
			<forward name="common.failure" path="/common/error.jsp"  />
		</action>

 
	               
	               
<!-- Admin User Registration to Services -->
	
		<action path="/ext/srv_adminregistrations/list" type="com.ext.portlet.srv.registrations.admin.ListSrvServiceRegistrationsAction" scope="request">
			<forward name="common.success" path="portlet.ext.srv.srv_adminregistrations.list"  />
			<forward name="common.failure" path="/common/error.jsp"  />
