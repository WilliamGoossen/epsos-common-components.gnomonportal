		<action path="/ext/srv_adminregistrations/update" type="com.ext.portlet.srv.registrations.admin.UpdateSrvServiceRegistrationAction"
		    name="SrvServiceAdminRegistrationForm" validate="false" scope="request">
			<forward name="/ext/srv_adminregistrations/list" path="/ext/srv_adminregistrations/list" redirect="true"/>
			<forward name="common.validation" path="portlet.ext.srv.srv_adminregistrations.form"  />
			<forward name="common.failure" path="/common/error.jsp"  />
		</action>

		<action path="/ext/srv_adminregistrations/delete" type="com.ext.portlet.srv.registrations.admin.DeleteSrvServiceRegistrationAction"
		    name="SrvServiceAdminRegistrationForm" validate="false" scope="request">
			<forward name="/ext/srv_adminregistrations/list" path="/ext/srv_adminregistrations/list" redirect="true"/>
			<forward name="common.validation" path="portlet.ext.srv.srv_adminregistrations.form"  />
			<forward name="common.failure" path="/common/error.jsp"  />
		</action>

<!-- SERVICE REPORTS -->




<action path="/ext/srv_reports/list" type="com.ext.portlet.srv.reports.ListSrvServiceReportsAction" scope="request">
			<forward name="common.success" path="portlet.ext.srv.reports.list"  />
