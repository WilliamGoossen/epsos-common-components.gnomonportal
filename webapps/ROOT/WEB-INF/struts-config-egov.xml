			scope="request"
			validate="false">
		<forward name="portlet.ext.egov.submit" path="portlet.ext.egov.tax.submitDebitFormView" />
		<forward name="portlet.ext.egov.pay" path="portlet.ext.egov.tax.payDebitFormView" />
		<forward name="portlet.ext.egov.failure" path="portlet.ext.egov.failure" />
    </action>

        <action path="/ext/e-services/tax/viewPaidTaxAction"
    		type="com.ext.portlet.egov.tax.ViewPaidTaxAction"
    		name="EgovDebtForm"
			scope="request"
			validate="false" >
		<forward name="portlet.ext.egov.tax.viewPaidTax" path="portlet.ext.egov.tax.viewPaidTax"/>
		<forward name="portlet.ext.egov.failure" path="/common/error.jsp" />
    </action>


     <action path="/ext/e-services/tax/calculatePeriodAmountAction"
    		type="com.ext.portlet.egov.tax.CalculatePeriodAmountAction"
    		name="EgovDebtForm"
